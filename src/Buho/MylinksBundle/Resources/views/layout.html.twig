{% extends '::base.html.twig' %}
{% block stylesheets %}
    {{ parent() }}
    <link href="{{ asset('bundles/buhomylinks/css/blueprint/screen.css') }}" type="text/css" rel="stylesheet" media="screen, projection">
    <link href="{{ asset('bundles/buhomylinks/css/blueprint/print.css') }}" type="text/css" rel="stylesheet" media="print"> 
    <!--[if lt IE 8]>
      <link rel="stylesheet" href="{{ asset('bundles/buhomylinks/css/blueprint/ie.css') }}" type="text/css" media="screen, projection">
    <![endif]-->
    <link href="{{ asset('bundles/buhomylinks/css/custom/layout.css') }}" type="text/css" rel="stylesheet" media="screen, projection">
{% endblock stylesheets %}
{% block title %} MyLinks {% endblock title %}


{% block body %} 
<div class="container">
    <div class="span-24">
        {% block header %}
        <div class="horizontal-nav">
            <ul>
                <li><a href="#">Home</a></li>
                <li><a href="#">Categories</a></li>
                <li><a href="#">Best Of</a></li>
                <li><a href="#">Contact</a></li>
            </ul>
        </div>
        <hr>
        <h1>
            <span class="small quiet">HTTP://</span><em>MyLinks</em>
        </h1>
        <h3>
        {% block header_title %} Title {% endblock header_title %}
        </h3>
        <hr>
        {% endblock header %}
    </div>
    
    <div class="span-4">
        {% block left %}
           <h4>navigation var</h4>
           <nav>
           
               <li><a href="#">some link</a></li>
               <li><a href="#">some link 2</a></li>
               <li><a href="#">some link 3</a></li>
           
           </nav>
        {% endblock left %}
    </div>
    <div class="span-16">
        {% block center %}center block{% endblock center %}
    </div>
    <div class="span-4 last">
        {% block right %}
           <h4>Best users</h4>
           <ul>
               <li>some link</li>
               <li>some link 2</li>
               <li>some link 3</li>
           </ul>
        {% endblock right %}
    </div>
    
    <div class="span-24">
        {% block footer %}footer{% endblock footer %}
    </div>
</div>
{% endblock body %}